for dir in $sysnames; do
  case $dir in
    /*) dest=$dir ;;
    *)  dest=$srcdir/$dir ;;
  esac
  if test -r $dest/configure; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: running configure fragment for $dir" >&5
printf "%s\n" "running configure fragment for $dir" >&6; }
    . $dest/configure
  fi

  if test -z "$uname"; then
    if test -r $dest/uname.c ||
       test -r $dest/uname.S ||
       { test -r $dest/syscalls.list &&
	 grep '^uname[ 	]' $dest/syscalls.list >/dev/null; }; then
      uname=$dir
    fi
  fi
done

